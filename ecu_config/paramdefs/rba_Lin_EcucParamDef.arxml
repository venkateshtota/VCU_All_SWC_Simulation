                            <L-4 L="EN">Standard</L-4>
                          </LONG-NAME>
                          <ORIGIN>RB:v16.0.1:2015-02-28</ORIGIN>
                        </ECUC-ENUMERATION-LITERAL-DEF>
                      </LITERALS>
                    </ECUC-ENUMERATION-PARAM-DEF>
